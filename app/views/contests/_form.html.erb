<%= simple_form_for(@contest) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title, :placeholder=>"Title" %>
    <%= f.input :description, :placeholder=>"Describe the contest/giveaway" %>
    <%= f.input :end_date%>
    <%= f.association :events, label_method: :name,collection: Festival.all(order: 'name')%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
