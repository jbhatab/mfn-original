@font-face {
    font-family: 'StereofidelicRegular';
    src: url(<%= asset_path'stereofidelic-webfont.eot'%>);
    src: url(<%= asset_path'stereofidelic-webfont.eot?#iefix'%>) format('embedded-opentype'),
         url(<%= asset_path'stereofidelic-webfont.woff'%>) format('woff'),
         url(<%= asset_path'stereofidelic-webfont.ttf'%>) format('truetype'),
         url(<%= asset_path'stereofidelic-webfont.svg#StereofidelicRegular'%>) format('svg');
    font-weight: normal;
    font-style: normal;
}
